  "vim.insertModeKeyBindings": [{
    "before": ["j", "k"],
    "after": ["<Esc>"]
  }],
  "vim.neovimPath": "/usr/local/bin/nvim",
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.leader": "\\",
  "vim.surround": true,
  "vim.enableNeovim": true,
  "editor.tabSize": 2,
  "terminal.external.osxExec": "iTerm.app",
  "editor.lineNumbers": "relative",
  "files.associations": {
    "*.def": "dotjs",
    "*.jst": "dotjs"
  },
  "beautify.config": {
    "indent_size": 2,
    "indent_char": " ",
    "end_with_newline": true
  },
  "sync.gist": "2cf4ace399e1d40c26696aece8d1dbfb",
  "sync.lastUpload": "2018-08-17T07:23:17.319Z",
  "sync.autoDownload": false,
  "sync.autoUpload": false,
  "sync.lastDownload": "",
  "sync.forceDownload": false,
  "sync.host": "",
  "sync.pathPrefix": "",
  "sync.quietSync": false,
  "sync.askGistName": false,
  "editor.renderWhitespace": "boundary",
  "files.trimTrailingWhitespace": true,
  "workbench.startupEditor": "newUntitledFile",
  "git.confirmSync": false,
  "dateTime.showDayOfMonth": true,
  "dateTime.showMonth": true,
  "editor.smoothScrolling": true,
  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 80,
  "window.title": "${dirty}${activeEditorShort}${separator}${rootName}${separator}",
  "window.restoreWindows": "one",
  "editor.mouseWheelScrollSensitivity": 2,
  "zenMode.hideTabs": false,
  "zenMode.restore": true,
  "zenMode.hideStatusBar": false,
  "workbench.activityBar.visible": false,
  "gitlens.advanced.messages": {
    "suppressNoRepositoryWarning": true,
    "suppressResultsExplorerNotice": true,
    "suppressShowKeyBindingsNotice": true,
    "suppressWelcomeNotice": true
  },
  "workbench.editor.showTabs": true,
  "terminal.integrated.shell.osx": "/usr/local/bin/zsh",
  "emmet.triggerExpansionOnTab": true,
  "window.zoomLevel": 0,
  "vim.statusBarColorControl": false,
  "workbench.statusBar.feedback.visible": false,
  "workbench.colorCustomizations": {
    "editor.selectionBackground": "#1c3f77",
    "[Blink (rainglow)]": {
      "statusBar.background": "#242424",
      "statusBar.foreground": "#a1a1a1",
    },
    "[Default Dark+]": {
      "statusBar.background": "#242424",
      "statusBar.foreground": "#a1a1a1",
    },
    "[Visual Studio Dark]": {
      "statusBar.background": "#242424",
      "statusBar.foreground": "#a1a1a1",
    },
    "[Horizon]": {
      "editorCursor.foreground": "#a1a1a1",
      "editorLineNumber.foreground": "#404040",
    },
    "[Laravel (rainglow)]": {
      "statusBar.background": "#242424",
      "statusBar.foreground": "#a1a1a1",
    },
  },
  "gitlens.keymap": "alternate",
  "zenMode.centerLayout": false,
  "sync.removeExtensions": true,
  "sync.syncExtensions": true,
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.iconTheme": "file-icons",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "never",
  "gitlens.advanced.caching.enabled": false,
  "gitlens.blame.heatmap.enabled": false,
  "search.exclude": {
    "**/www": true
  },
  "java.errors.incompleteClasspath.severity": "ignore",
  "editor.fontWeight": "500",
  "gitlens.views.fileHistory.enabled": true,
  "gitlens.views.lineHistory.enabled": true,
  "editor.quickSuggestions": {
    "other": false,
    "comments": false,
    "strings": false
  },
  "breadcrumbs.enabled": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorTheme": "Blink (rainglow)",
}
